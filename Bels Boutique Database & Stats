SELECT * FROM belsboutique;

SELECT * FROM belsboutique ORDER BY price;
SELECT * FROM belsboutique ORDER BY quantity;

SELECT SUM(quantity) FROM belsboutique;

SELECT SUM(price) FROM belsboutique GROUP BY small;
SELECT SUM(price) FROM belsboutique;
SELECT MIN(price) FROM belsboutique GROUP BY medium;
SELECT MIN(price) FROM belsboutique;
SELECT MAX(price) FROM belsboutique GROUP BY large;
SELECT MAX(price) FROM belsboutique;

