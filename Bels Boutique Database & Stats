INSERT INTO belsboutique VALUES (8, "trousers bootcut", 4, 4, 4, 12, 50);
INSERT INTO belsboutique VALUES (9, "trousers short", 4, 4, 4, 12, 40);
INSERT INTO belsboutique VALUES (10, "skirt midlength", 2, 2, 2, 6, 50);
